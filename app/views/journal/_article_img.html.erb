<% if picture_url.size > 0 %>
    <div style="width:100%; border-style:solid;border-width:5px">
      <a class="fancy_box_cont" href="<%= picture_url %>">
        <img class="img-responsive" src="<%= get_thumbnail(picture_url) %>" alt="This picture loads on non-supporting browsers.">
      </a>
    </div>
<% end %>